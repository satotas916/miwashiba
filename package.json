{
  "name": "package",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=develop npm-run-all dist -p server watch",
    "watch": "watch \"npm run dist\" src/",
    "dist": "npm-run-all dist:**",
    "dist:ejs-top": "ejs-cli --base-dir src/ejs/ '/index.ejs' --out dist/",
    "dist:ejs-gallery": "ejs-cli --base-dir src/ejs/ '/gallery/index.ejs' --out dist/",
    "dist:ejs-works": "ejs-cli --base-dir src/ejs/ '/works/index.ejs' --out dist/",
    "dist:ejs-other": "ejs-cli --base-dir src/ejs/ '/other/index.ejs' --out dist/",
    "dist:sass": "node-sass src/scss/ '/**/*.scss' --output dist/css/ --output-style compressed",
    "dist:font": "cpx 'src/static/font/*' 'dist/font/'",
    "dist:js": "cpx 'src/static/js/*' 'dist/js/'",
    "dist:img": "cpx 'src/static/img/*' 'dist/img/'",
    "build": "npm-run-all build:**",
    "build:ejs-top": "ejs-cli --base-dir src/ejs/ '/index.ejs' --out dist/",
    "build:ejs-gallery": "ejs-cli --base-dir src/ejs/ '/gallery/index.ejs' --out dist/",
    "build:ejs-works": "ejs-cli --base-dir src/ejs/ '/works/index.ejs' --out dist/",
    "build:ejs-other": "ejs-cli --base-dir src/ejs/ '/other/index.ejs' --out dist/",
    "build:sass": "node-sass src/scss/ '/**/*.scss' --output production/css/ --output-style compressed",
    "build:font": "cpx 'src/static/font/*' 'dist/font/'",
    "build:js": "cpx 'src/static/js/*' 'dist/js/'",
    "build:img": "cpx 'src/static/img/*' 'dist/img/'",
    "server": "browser-sync start --server './dist/' --files './dist/**'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/satotas916/risuke.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/satotas916/risuke/issues"
  },
  "homepage": "https://github.com/satotas916/risuke#readme",
  "dependencies": {
    "concurrently": "^5.0.0",
    "cpx": "^1.5.0",
    "ejs-cli": "^2.1.1",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "fs": "0.0.1-security",
    "path": "^0.12.7"
  }
}
